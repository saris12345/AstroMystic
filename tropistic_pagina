    <script>
        // Crear estrellas animadas
        function createStars() {
            const starsContainer = document.getElementById('stars');
            const numberOfStars = 50;
            
            for (let i = 0; i < numberOfStars; i++) {
                const star = document.createElement('div');
                star.className = 'star';
                star.style.left = Math.random() * 100 + '%';
                star.style.top = Math.random() * 100 + '%';
                star.style.width = Math.random() * 3 + 1 + 'px';
                star.style.height = star.style.width;
                star.style.animationDelay = Math.random() * 3 + 's';
                starsContainer.appendChild(star);
            }
        }

        // Navegaci√≥n suave
        function scrollToSection(sectionId) {
            document.getElementById(sectionId).scrollIntoView({
                behavior: 'smooth'
            });
        }

        // Variables globales para el servicio
        let currentService = '';
        let currentFormData = {};
        
        // Precios de los servicios
        const servicePrices = {
            'Carta Natal': 45,
            'Predicci√≥n Anual': 65,
            'Compatibilidad': 55,
            '3 Preguntas': 35
        };

        // Abrir formulario de servicio
        function openServiceForm(serviceName) {
            currentService = serviceName;
            document.getElementById('serviceTitle').textContent = serviceName;
            
            // Ocultar todos los campos espec√≠ficos
            document.getElementById('compatibilityFields').classList.add('hidden');
            document.getElementById('questionsFields').classList.add('hidden');
            
            // Mostrar campos espec√≠ficos seg√∫n el servicio
            if (serviceName === 'Compatibilidad') {
                document.getElementById('compatibilityFields').classList.remove('hidden');
                // Hacer campos requeridos
                document.querySelector('input[name="nombre2"]').required = true;
                document.querySelector('input[name="fecha2"]').required = true;
                document.querySelector('input[name="ciudad2"]').required = true;
            } else if (serviceName === '3 Preguntas') {
                document.getElementById('questionsFields').classList.remove('hidden');
                // Hacer preguntas requeridas
                document.querySelector('textarea[name="pregunta1"]').required = true;
                document.querySelector('textarea[name="pregunta2"]').required = true;
                document.querySelector('textarea[name="pregunta3"]').required = true;
            } else {
                // Para Carta Natal y Predicci√≥n Anual, quitar required de campos espec√≠ficos
                document.querySelector('input[name="nombre2"]').required = false;
                document.querySelector('input[name="fecha2"]').required = false;
                document.querySelector('input[name="ciudad2"]').required = false;
                document.querySelector('textarea[name="pregunta1"]').required = false;
                document.querySelector('textarea[name="pregunta2"]').required = false;
                document.querySelector('textarea[name="pregunta3"]').required = false;
            }
            
            document.getElementById('serviceModal').classList.remove('hidden');
            document.getElementById('serviceModal').classList.add('flex');
        }

        // Cerrar formulario de servicio
        function closeServiceForm() {
            document.getElementById('serviceModal').classList.add('hidden');
            document.getElementById('serviceModal').classList.remove('flex');
            document.getElementById('serviceForm').reset();
        }

        // Manejar env√≠o de formulario de servicio
        function handleServiceForm(event) {
            event.preventDefault();
            
            const formData = new FormData(event.target);
            currentFormData = {
                servicio: currentService,
                nombre: formData.get('nombre'),
                email: formData.get('email'),
                fecha: formData.get('fecha'),
                hora: formData.get('hora'),
                ciudad: formData.get('ciudad'),
                comentarios: formData.get('comentarios')
            };

            // Datos adicionales seg√∫n el servicio
            if (currentService === 'Compatibilidad') {
                currentFormData.nombre2 = formData.get('nombre2');
                currentFormData.fecha2 = formData.get('fecha2');
                currentFormData.hora2 = formData.get('hora2');
                currentFormData.ciudad2 = formData.get('ciudad2');
            } else if (currentService === '3 Preguntas') {
                currentFormData.pregunta1 = formData.get('pregunta1');
                currentFormData.pregunta2 = formData.get('pregunta2');
                currentFormData.pregunta3 = formData.get('pregunta3');
            }

            // Mostrar modal de pago
            showPaymentModal();
        }

        // Mostrar modal de m√©todos de pago
        function showPaymentModal() {
            const price = servicePrices[currentService];
            document.getElementById('finalPrice').textContent = '‚Ç¨' + price;
            
            // Ocultar modal de servicio y mostrar modal de pago
            document.getElementById('serviceModal').classList.add('hidden');
            document.getElementById('paymentModal').classList.remove('hidden');
            document.getElementById('paymentModal').classList.add('flex');
        }

        // Cerrar modal de pago
        function closePaymentModal() {
            document.getElementById('paymentModal').classList.add('hidden');
            document.getElementById('paymentModal').classList.remove('flex');
        }

        // Volver al formulario desde el pago
        function goBackToForm() {
            closePaymentModal();
            document.getElementById('serviceModal').classList.remove('hidden');
            document.getElementById('serviceModal').classList.add('flex');
        }

        // Seleccionar m√©todo de pago
        function selectPaymentMethod(method) {
            const methodNames = {
                'paypal': 'PayPal',
                'tarjeta': 'Tarjeta de Cr√©dito/D√©bito',
                'transferencia': 'Transferencia Bancaria',
                'bizum': 'Bizum'
            };

            const price = servicePrices[currentService];
            
            let mensaje = `¬°Solicitud completada con √©xito!\n\n`;
            mensaje += `Servicio: ${currentFormData.servicio}\n`;
            mensaje += `Precio: ‚Ç¨${price}\n`;
            mensaje += `M√©todo de pago: ${methodNames[method]}\n`;
            mensaje += `Cliente: ${currentFormData.nombre}\n`;
            mensaje += `Email: ${currentFormData.email}\n\n`;
            
            if (currentService === '3 Preguntas') {
                mensaje += `Tus preguntas han sido registradas y ser√°n respondidas por nuestra astr√≥loga experta.\n\n`;
            } else if (currentService === 'Compatibilidad') {
                mensaje += `Analizaremos la compatibilidad entre ${currentFormData.nombre} y ${currentFormData.nombre2}.\n\n`;
            }
            
            mensaje += `Recibir√°s las instrucciones de pago y tu consulta astrol√≥gica personalizada en tu email en un plazo de 24-48 horas.\n\n`;
            mensaje += `¬°Gracias por confiar en Astrum!`;
            
            alert(mensaje);
            
            // Cerrar todos los modales y limpiar datos
            closePaymentModal();
            closeServiceForm();
            currentFormData = {};
        }



        // Manejar formulario de contacto
        function handleContactForm(event) {
            event.preventDefault();
            alert('¬°Mensaje enviado con √©xito! Te responderemos en un plazo de 24 horas.');
            event.target.reset();
        }

        // Navegaci√≥n suave para enlaces del header
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Cerrar modales al hacer clic fuera
        document.getElementById('serviceModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeServiceForm();
            }
        });

        document.getElementById('paymentModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closePaymentModal();
            }
        });

        // Inicializar estrellas al cargar la p√°gina
        createStars();
    </script>
